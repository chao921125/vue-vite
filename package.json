 "author": "vue-next-vite",
 "homepage": "https://github.com/vitejs/awesome-vite#plugins",
 "main": "vue-next-vite",
 "repository": {
  "type": "git",
  "url": "https://github.com/vitejs/awesome-vite#plugins"
 },
 "bugs": {
  "url": "https://github.com/vitejs/awesome-vite#plugins"
 },
 "browserslist": [
  "defaults",
  "> 1%",
  "last 2 versions",
  "not ie 11",
  "not op_mini all"
 ],
 "engines": {
  "node": "^12 || >=14"
 },
 "lint-staged": {
  "*.{js,jsx,ts,tsx}": [
   "eslint --fix",
   "prettier --write"
  ],
  "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
   "prettier --write--parser json"
  ],
  "package.json": [
   "prettier --write"
  ],
  "*.vue": [
   "stylelint --fix",
   "eslint --fix",
   "prettier --write"
  ],
  "*.{scss,less,styl,html}": [
   "stylelint --fix",
   "prettier --write"
  ],
  "*.md": [
   "prettier --write"
  ]
 },
 "config": {
  "commitizen": {
   "path": "node_modules/cz-git"
  }
 }
}
