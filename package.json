	"main": "vue-vite",
	"types": "vue-vite",
	"type": "module",
	"scripts": {
		"review": "npm run build && npm run dev",
		"serve": "npm run dev",
		"dev": "vite --host --open --mode development",
		"test": "vite --host --open --mode preview",
		"prod": "vite --host --open --mode production",
		"build": "npm run build:prod",
		"build:dev": "vue-tsc --noEmit --skipLibCheck && vite build --mode development",
		"build:test": "vue-tsc --noEmit --skipLibCheck && vite build --mode preview",
		"build:prod": "vue-tsc --noEmit --skipLibCheck && vite build --mode production",
		"build:ig": "vite build --mode production",
		"build:no-cache": "npm clean:cache && npm run build",
		"clean:cache": "rimraf yarn.lock && rimraf package.lock.json && rimraf pnpm-lock.yaml && rimraf node_modules && pnpm install",
		"clean:lib": "rimraf node_modules",
		"lint:lint-staged": "lint-staged",
		"lint:prettier": "prettier --write --fix --loglevel warn \"./**/*.{js,ts,json,jsx,tsx,css,less,scss,vue,html}\"",
		"lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"prepare": "npx husky install",
		"preview": "vite preview",
		"upgrade": "ncu -u && pnpm install",
		"test:br": "http-server dist --cors --brotli -c-1",
		"test:gzip": "http-server dist --cors --gzip -c-1",
		"commitlint": "commitlint --edit"
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix",
		"*.ts": "eslint --cache --fix",
		"*.scss": "stylelint --fix",
		"*.less": "stylelint --fix",
		"*.css": "stylelint --fix"
	},
