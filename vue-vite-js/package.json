{
  "name": "vue-vite-js",
  "version": "3.0.0",
  "description": "vue-vite",
  "keywords": [
    "vue, vite"
  ],
  "homepage": "https://github.com/chao921125/vue-vite",
  "repository": {
    "type": "git",
    "url": "https://github.com/chao921125/vue-vite"
  },
  "bugs": {
    "url": "https://github.com/chao921125/vue-vite/issues"
  },
  "author": {
    "name": "chao921125",
    "email": "chao921125@sina.com",
    "url": "https://github.com/chao921125"
  },
  "license": "BSD-3-Clause",
  "main": "index.html",
  "type": "module",
  "private": true,
  "scripts": {
    "review": "npm run build && npm run dev",
    "serve": "npm run dev",
    "dev": "vite --host --open --mode development",
    "test": "vite --host --open --mode preview",
    "prod": "vite --host --open --mode production",
    "build": "npm run build:prod",
    "build:test": "vite build --mode preview",
    "build:prod": "vite build --mode production",
    "build:no-cache": "npm clean:cache && npm run build",
    "clean:cache": "rimraf yarn.lock && rimraf package.lock.json && rimraf pnpm-lock.yaml && rimraf node_modules && pnpm install",
    "clean:lib": "rimraf node_modules",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint--config eslint.config.mjs \"./**/*.{js,ts,mjs,mts,cjs,cts,jsx,tsx,css,less,scss,vue,html}\"",
    "lint:prettier": "prettier --write --fix --loglevel warn \"./**/*.{js,ts,mjs,mts,cjs,cts,json,jsx,tsx,css,less,scss,vue,html}\"",
    "lint:lint-staged": "lint-staged",
    "lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,css,less,scss,postcss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prepare": "npx husky install",
    "commitlint": "commitlint --edit",
    "prepack": "npmignore --auto",
    "test:br": "http-server dist --cors --brotli -c-1",
    "test:gzip": "http-server dist --cors --gzip -c-1"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.1",
    "nprogress": "^0.2.0",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@eslint/js": "^9.9.1",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/compiler-sfc": "^3.4.38",
    "browserslist": "^4.23.3",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.5",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "terser": "^5.31.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-icons": "^0.19.2",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-define-options": "^1.4.9",
    "vite": "^5.4.2",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.20.1",
    "vite-plugin-restart": "^0.4.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-top-level-await": "^1.4.4",
    "vue-tsc": "^2.0.29"
  },
  "peerDependencies": {
    "mysql2": ">=3.3.2"
  },
  "overrides": {
    "mysql": "2.18.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.14.18",
    "pnpm": ">=6.20.0",
    "yarn": ">=3.0.0"
  }
}
